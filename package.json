{
  "name": "rehype-react",
  "description": "Compile Hast tree to React with remark",
  "version": "3.1.0",
  "keywords": [
    "compile",
    "html",
    "markdown",
    "remark",
    "stringify"
  ],
  "license": "MIT",
  "repository": "https://github.com/rhysd/rehype-react",
  "bugs": "https://github.com/rhysd/rehype-react/issues",
  "author": [
    {
      "name": "Titus Wormer",
      "email": "tituswormer@gmail.com"
    },
    {
      "name": "rhysd",
      "email": "lin90162@yahoo.co.jp"
    }
  ],
  "contributors": [
    "Tom MacWright <tom@macwright.org>",
    "Titus Wormer <tituswormer@gmail.com>",
    "Ciaran Wood <cyrzinsomnia@gmail.com>",
    "Juho Vepsalainen <bebraw@gmail.com>",
    "Jason Trill <jason@jasontrill.com>",
    "Tsuyusato Kitsune <make.just.on@gmail.com>",
    "Jeremy Stucki <jeremy@interactivethings.com>",
    "rhysd <lin90162@yahoo.co.jp>"
  ],
  "dependencies": {
    "@mapbox/hast-util-table-cell-style": "^0.1.3",
    "hast-to-hyperscript": "^5.0.0"
  },
  "devDependencies": {
    "hastscript": "^3.1.0",
    "nyc": "^11.3.0",
    "prettier": "^1.18.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "remark": "^8.0.0",
    "remark-cli": "^4.0.0",
    "remark-preset-wooorm": "^3.0.0",
    "tape": "^4.8.0",
    "unified": "^6.1.6",
    "unist-builder": "^1.0.2",
    "xo": "^0.24.0"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run format && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
